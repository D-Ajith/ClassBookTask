{
  "version": 2,
  "builds": [
    {
      "src": "classbook-frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "classbook-frontend/dist",
        "installCommand": "cd classbook-frontend && npm install",
        "buildCommand": "cd classbook-frontend && npm run build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "classbook-frontend/index.html"
    }
  ]
}